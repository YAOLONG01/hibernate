<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
        <hibernate-mapping>
        	<class name="cn.hjycjc.spring.hibernate.helloworld.n21.both.Customer" table="customer_table">
        		<!--特殊  -->
        		<id name="customerId" type="java.lang.Integer">
        			<column name="customer_id"></column>
        			<generator class="native"></generator>
        		</id>
        		
        		<property name="customerName" type="java.lang.String">
        			<column name="customer_name"></column>
        		</property>
        		
        		<!-- 12n -->
        		<set name="orders" table="Order_Table">
        			<key column="customer_id"></key>
        			<one-to-many class="cn.hjycjc.spring.hibernate.helloworld.n21.both.Order"></one-to-many>
        		</set>
        		
        		
        	</class>
        </hibernate-mapping>